{
  "name": "promise-queue",
  "version": "1.0.0",
  "description": "Turn an observable into a promise queue (ie. to allow handling of observables within sagas)",
  "main": "index.js",
  "author": "MrNKR",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest"
  },
  "dependencies": {
    "@mrnkr/simple-queue": "^1.0.5",
    "redux-saga-test-plan": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "jest": "^24.8.0",
    "redux-saga": "^1.0.2",
    "rxjs": "^6.5.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.5"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts)?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ]
  }
}
